{
  "name": "barberprofile-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:tunnel": "concurrently \"npm run dev\" \"npx localtunnel --port 3000\"",
    "dev:fixed": "concurrently -n \"vite,tunnel\" -c \"cyan,magenta\" \"npm run dev\" \"npx cloudflared tunnel --url http://localhost:3000\"",
    "dev:ngrok": "concurrently -n \"vite,ngrok\" -c \"cyan,magenta\" \"npm run dev\" \"npx ngrok http 3000\"",
    "dev:cloudflared:fixed": "concurrently -n \"vite,cloudflared\" -c \"cyan,magenta\" \"npm run dev\" \"npx cloudflared tunnel run\"",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "sharp": "^0.34.5",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
